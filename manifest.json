{
  "manifest_version": 2,

  "name": "ScholarMe",
  "description": "The Non-Google Scholar Google Scholar",
  "version": "1.0",

  "permissions": [
    "tabs",
    "fileBrowserHandler",
    "contextMenus",
    "storage"
  ],
  "browser_action": {
    "default_icon": "img/icon.png"
  },
  "background": {
    "page": "html/background.html"
  },
  "icons": {
    "16": "img/icon.png"
  }
}
